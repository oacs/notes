{
  "literatureNoteFolder": "sources",
  "zoteroDataDir": "/home/oacs/Zotero",
  "citationLibrary": 1,
  "autoRefresh": true,
  "symlinkImgExcerpt": false,
  "imgExcerptPath": "ZtImgExcerpt",
  "level": "INFO",
  "template": {
    "ejected": true,
    "folder": "extra/templates/ZtTemplates",
    "templates": {
      "filename": "<%= it.title %>.md",
      "citation": "[<%= it.title %>]",
      "altCitation": "@<%= it.title %>"
    }
  },
  "fmFieldsMode": "whitelist",
  "fmFieldsMapping": {
    "title": true,
    "citekey": true
  },
  "fmTagPrefix": "",
  "autoPairEta": false,
  "updateAnnotBlock": false,
  "updateOverwrite": false,
  "citationEditorSuggester": true,
  "showCitekeyInSuggester": true,
  "noteFields": {
    "highlights": {
      "keyword": "",
      "template": ""
    },
    "questions": {
      "keyword": "",
      "template": ""
    }
  },
  "noteFieldsSuggester": false,
  "enableServer": true,
  "serverPort": 9091
}